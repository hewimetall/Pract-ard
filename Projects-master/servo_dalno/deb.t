//f for ratate servo 0-180 deg or dalnom , which write on P.d .
void rotate(){
  for(int i = 0; i < 180; i++) {
    /* code */
     servo.write(i); // передает значения для вала сервопривода
     dalnom();
     delay(10);
   }
}
// f step return on position min dalnom
void rotateReturn(unsigned long t=1){
  rotate();
  ///// trace P.tic
